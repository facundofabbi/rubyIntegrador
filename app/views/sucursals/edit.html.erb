<button type="button" class="btn btn-outline-dark"><%= link_to 'Volver al listado', sucursals_path %></button>
<h2>Editar Sucursal</h2>
<%= form_for @sucursal, url: update_sucursal_path(@sucursal)  do |form| %>

    <% if !@sucursal.nil? %>
        <% @sucursal.errors.full_messages.each do |error| %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% end %>
    <% end %>

    <div>
        <%= form.label :nombre %>
        <%= form.text_field :nombre%>
    </div>

    <div>
        <%= form.label :direccion %>
        <%= form.text_field :direccion%>
    </div>

    <div>
        <%= form.label :telefono %>
        <%= form.text_field :telefono%>
    </div>

    

    <%= form.submit "Editar" %>


<%end%>
