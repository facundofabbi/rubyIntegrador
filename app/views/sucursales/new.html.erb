<button type="button" class="btn btn-outline-dark"><%= link_to 'Volver al listado', sucursales_path %></button>
<h2>Agregar una nueva Sucursal</h2>


<%= form_with model: @sucursales do |form| %>

    <div class="form-group">
        <%= form.label :nombre %>
        <%= form.text_field :nombre%>
    </div>

    <div class="form-group">
        <%= form.label :direccion %>
        <%= form.text_field :direccion%>
    </div>
    
    <div class="form-group">
        <%= form.label :telefono %>
        <%= form.number_field :telefono%>
    </div>

    <%= form.submit 'guardar' %>


<%end%>