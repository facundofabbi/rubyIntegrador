<button type="button" class="btn btn-outline-dark"><%= link_to 'Volver al listado', sucursales_path %></button>
<h2>Editar Sucursal</h2>
<%= form_with model: @sucursal do |form| %>

    <div>
        <%= form.label :nombre %>
        <%= form.text_field :nombre%>
    </div>

    <div>
        <%= form.label :direccion %>
        <%= form.text_field :direccion%>
    </div>

    <div>
        <%= form.label :telefono %>
        <%= form.text_field :telefono%>
    </div>

    

    <%= form.submit %>


<%end%>
