<h2>Editar usuario</h2>

<%= link_to 'Volver al listado', usuario_path %>

<%= form_with model: @usuario do |form| %>

    <div>
        <%= form.label :nombre %>
        <%= form.text_field :nombre%>
    </div>

    <div>
        <%= form.label :email %>
        <%= form.text_field :email%>
    </div>

    <div>
        <%= form.label :Role %>
        <%= form.select :role, ["Administrador", "Personal Bancario", "cliente"] %>
    </div>
    

    <%= form.submit %>


<%end%>